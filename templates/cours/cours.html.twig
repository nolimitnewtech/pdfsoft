              <!-- /.card-header -->
              <!-- form start -->
              <form role="form" method="post" action="" enctype="multipart/form-data">
                <div class="card-body" class="col-lg-12">
                  <div class="form-group">
                    <label for="exampleInputEmail1">Titre du cours</label>
                    <input type="text" name="titre_cours" class="form-control" id="exampleInputEmail1" placeholder="Entre le titre du cours">
                  </div>
                  <div class="form-group">
                    <label>Filiere du cours</label>
                    <select name="filiere_cours" class="form-control">
