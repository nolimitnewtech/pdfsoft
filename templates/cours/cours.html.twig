                      <option value="niv 2">niv 2</option>
                      <option value="niv 3">niv 3</option>
                      <option value="niv 4">niv 4</option>
                      <option value="niv 5">niv 5</option>
                    </select>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputFile">Choix du cours</label>
                    <div class="input-group">
                      <div class="custom-file">
                        <input type="file" name="fichier_cours" class="custom-file-input" id="exampleInputFile">
                        <label class="custom-file-label" for="exampleInputFile">Choisi le fichier du cours</label>
                      </div>
                      <div class="input-group-append">
                        <span class="input-group-text" id=""></span>
                      </div>
                    </div>
                  </div>
                  <div class="form-group">
                    <label>Description du cours</label>
                    <textarea name="Description_cours" class="form-control" rows="3" placeholder="Decrivez le cours en peu de ligne"></textarea>
                  </div>
                  
                </div>
                <!-- /.card-body -->

                <div class="card-footer">
                  <button type="submit" class="btn btn-primary btn-lg">Envoyer le cours au serveur <span class="fa fa-send"></span></button>
